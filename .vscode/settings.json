{
  "json.schemas": [
    {
      "fileMatch": [
        "lib/**/*.json"
      ],
      "url": "http://json.schemastore.org/eslintrc"
    }
  ],
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.eslintIntegration": true,
  "prettier.requireConfig": true,
  "cSpell.words": [
    "singleline"
  ]
}
